{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "react-icons/sf": ["./src/icons/sf"],
      "react-icons/sf6": ["./src/icons/sf6"]
